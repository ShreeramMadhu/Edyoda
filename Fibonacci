def fibonacci(n):
    fib_sequence = [0, 1]
    for i in range(2, n):
        next_num = fib_sequence[i - 1] + fib_sequence[i - 2]
        fib_sequence.append(next_num)
    return fib_sequence

n = 9  # Change this value to get more terms if needed
fib_sequence = fibonacci(n)
output = " ".join(map(str, fib_sequence))
print(output)








